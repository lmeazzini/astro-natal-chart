# Production Environment Variables for Docker Compose
# Copy this to .env.prod and update with actual values
# IMPORTANT: Never commit .env.prod to git!

# Domain Configuration
DOMAIN=example.com

# Database Configuration
POSTGRES_DB=astro_prod
POSTGRES_USER=astro
POSTGRES_PASSWORD=CHANGE_ME_STRONG_PASSWORD_HERE

# Redis Configuration
REDIS_PASSWORD=CHANGE_ME_REDIS_PASSWORD_HERE

# These variables are used by docker-compose.prod.yml
# Additional app-specific variables should be in apps/api/.env
